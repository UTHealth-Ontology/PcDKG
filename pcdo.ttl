<https://raw.githubusercontent.com/UTHealth-Ontology/PcDO/main/pcdo.owl> rdf:type owl:Ontology ;
                                                                          owl:versionIRI <http://sbmi.uth.edu/ontology/pcdo/2.0> ;
                                                                          owl:imports <http://www.w3.org/2004/02/skos/core> ;
                                                                          terms:contributor "Dave Beckett" ,
                                                                                            "Nikki Rogers" ,
                                                                                            "Participants in W3C's Semantic Web Deployment Working Group." ;
                                                                          terms:creator "Alistair Miles" ,
                                                                                        "Clifford Chen" ,
                                                                                        "Sean Bechhofer" ,
                                                                                        "Tuan Amith, Cui Tao" ;
                                                                          terms:description "An RDF vocabulary for describing the basic structure and content of concept schemes such as thesauri, classification schemes, subject heading lists, taxonomies, 'folksonomies', other types of controlled vocabulary, and also concept schemes embedded in glossaries and terminologies."@en ;
                                                                          terms:title "Patient centric Drug Ontology" ,
                                                                                      "SKOS Vocabulary"@en ;
                                                                          rdfs:seeAlso <http://www.w3.org/TR/skos-reference/> ;
                                                                          owl:versionInfo 2 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
terms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
terms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty .


###  http://sbmi.uth.edu/ontology/pcdo#natural_language
pcdo:natural_language rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty ;
              rdfs:comment "The range of skos:altLabel is the class of RDF plain literals."@en ,
                           "skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise disjoint properties."@en ;
              rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
              rdfs:label "alternative label"@en ;
              skos:definition "An alternative lexical label for a resource."@en ;
              skos:example "Acronyms, abbreviations, spelling variants, and irregular plural/singular forms may be included among the alternative labels for a concept. Mis-spelled terms are normally included as hidden labels (see skos:hiddenLabel)."@en ;
              rdfs:subPropertyOf rdfs:label .


###  http://www.w3.org/2004/02/skos/core#changeNote
skos:changeNote rdf:type owl:AnnotationProperty ;
                rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
                rdfs:label "change note"@en ;
                skos:definition "A note about a modification to a concept."@en ;
                rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty ;
                rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
                rdfs:label "definition"@en ;
                skos:definition "A statement or formal explanation of the meaning of a concept."@en ;
                rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#editorialNote
skos:editorialNote rdf:type owl:AnnotationProperty ;
                   rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
                   rdfs:label "editorial note"@en ;
                   skos:definition "A note for an editor, translator or maintainer of the vocabulary."@en ;
                   rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty ;
             rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
             rdfs:label "example"@en ;
             skos:definition "An example of the use of a concept."@en ;
             rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#hiddenLabel
skos:hiddenLabel rdf:type owl:AnnotationProperty ;
                 rdfs:comment "The range of skos:hiddenLabel is the class of RDF plain literals."@en ,
                              "skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise disjoint properties."@en ;
                 rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
                 rdfs:label "hidden label"@en ;
                 skos:definition "A lexical label for a resource that should be hidden when generating visual displays of the resource, but should still be accessible to free text search operations."@en ;
                 rdfs:subPropertyOf rdfs:label .


###  http://www.w3.org/2004/02/skos/core#historyNote
skos:historyNote rdf:type owl:AnnotationProperty ;
                 rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
                 rdfs:label "history note"@en ;
                 skos:definition "A note about the past state/use/meaning of a concept."@en ;
                 rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty ;
          rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
          rdfs:label "note"@en ;
          skos:definition "A general note, for any purpose."@en ;
          skos:scopeNote "This property may be used directly, or as a super-property for more specific note types."@en .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty ;
               rdfs:comment "A resource has no more than one value of skos:prefLabel per language tag, and no more than one value of skos:prefLabel without language tag."@en ,
                            "The range of skos:prefLabel is the class of RDF plain literals."@en ,
                            """skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise
      disjoint properties."""@en ;
               rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
               rdfs:label "preferred label"@en ;
               skos:definition "The preferred lexical label for a resource, in a given language."@en ;
               rdfs:subPropertyOf rdfs:label .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty ;
               rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
               rdfs:label "scope note"@en ;
               skos:definition "A note that helps to clarify the meaning and/or the use of a concept."@en ;
               rdfs:subPropertyOf skos:note .


#################################################################
#    Object Properties
#################################################################

###  http://sbmi.uth.edu/ontology/pcdo#ER_Warnings_for
pcdo:ER_Warnings_for rdf:type owl:ObjectProperty ;
                     rdfs:domain pcdo:ER_Warnings ;
                     rdfs:range pcdo:Special_Population .


###  http://sbmi.uth.edu/ontology/pcdo#action_in_response_to
pcdo:action_in_response_to rdf:type owl:ObjectProperty ;
                           owl:inverseOf pcdo:need_to_take_action ;
                           rdfs:domain pcdo:Special_instructions ;
                           rdfs:range pcdo:Reaction .


###  http://sbmi.uth.edu/ontology/pcdo#acts_by
pcdo:acts_by rdf:type owl:ObjectProperty ;
             rdfs:domain pcdo:Drug ;
             rdfs:range pcdo:Mechanism_of_Action .


###  http://sbmi.uth.edu/ontology/pcdo#administered_in
pcdo:administered_in rdf:type owl:ObjectProperty ;
                     rdfs:domain pcdo:Drug ;
                     rdfs:range pcdo:Route .


###  http://sbmi.uth.edu/ontology/pcdo#affects_body_system
pcdo:affects_body_system rdf:type owl:ObjectProperty ;
                         rdfs:domain pcdo:Drug ;
                         rdfs:range pcdo:Body_System .


###  http://sbmi.uth.edu/ontology/pcdo#affects_individuals_of
pcdo:affects_individuals_of rdf:type owl:ObjectProperty ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( pcdo:Cause
                                                        pcdo:Drug_Target
                                                        pcdo:Reaction
                                                      )
                                        ] ;
                            rdfs:range pcdo:Special_Population ;
                            rdfs:comment "Reaction -> People" .


###  http://sbmi.uth.edu/ontology/pcdo#after
pcdo:after rdf:type owl:ObjectProperty ;
           rdfs:domain pcdo:Dosage ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( pcdo:Date
                                      pcdo:Occurrence
                                    )
                      ] ;
           rdfs:comment "Dosage -> Date or Occurence" .


###  http://sbmi.uth.edu/ontology/pcdo#allergic_to
pcdo:allergic_to rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( pcdo:Allergic_Reaction
                                             pcdo:People_with_Allergic_Reaction
                                           )
                             ] ;
                 rdfs:range pcdo:Allergen ;
                 rdfs:comment "Allergic Reaction -> Allergen" .


###  http://sbmi.uth.edu/ontology/pcdo#approved_by
pcdo:approved_by rdf:type owl:ObjectProperty ;
                 rdfs:domain pcdo:Drug ;
                 rdfs:range pcdo:Organization .


###  http://sbmi.uth.edu/ontology/pcdo#associated
pcdo:associated rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( pcdo:Drug_Target
                                            pcdo:Outcome
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( pcdo:Drug_Target
                                           pcdo:Outcome
                                         )
                           ] .


###  http://sbmi.uth.edu/ontology/pcdo#avoid_taking_with
pcdo:avoid_taking_with rdf:type owl:ObjectProperty ;
                       rdfs:domain pcdo:Drug ;
                       rdfs:range pcdo:Drug_Combination .


###  http://sbmi.uth.edu/ontology/pcdo#before
pcdo:before rdf:type owl:ObjectProperty ;
            rdfs:domain pcdo:Dosage ;
            rdfs:range pcdo:Occurrence ;
            rdfs:comment "Dosage -> Occurrence" .


###  http://sbmi.uth.edu/ontology/pcdo#born_from
pcdo:born_from rdf:type owl:ObjectProperty ;
               rdfs:domain pcdo:People_Born_From ;
               rdfs:range pcdo:Date ;
               rdfs:comment "People Born From -> Date" .


###  http://sbmi.uth.edu/ontology/pcdo#builds_up_to
pcdo:builds_up_to rdf:type owl:ObjectProperty ;
                  rdfs:domain pcdo:Drug_Combination ;
                  rdfs:range pcdo:Reaction .


###  http://sbmi.uth.edu/ontology/pcdo#can_be_prevented_by
pcdo:can_be_prevented_by rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain pcdo:Adverse_Drug_Reactions ;
                         rdfs:range pcdo:Prevention_Measure .


###  http://sbmi.uth.edu/ontology/pcdo#cause_to_people
pcdo:cause_to_people rdf:type owl:ObjectProperty ;
                     rdfs:domain pcdo:Cause ;
                     rdfs:range pcdo:Special_Population ;
                     rdfs:comment "Cause->People" ;
                     owl:deprecated "true"^^xsd:boolean .


###  http://sbmi.uth.edu/ontology/pcdo#causes
pcdo:causes rdf:type owl:ObjectProperty ;
            rdfs:domain pcdo:Drug_Target ;
            rdfs:range pcdo:Cause ;
            skos:changeNote "change to \"drug target effect\"" .


###  http://sbmi.uth.edu/ontology/pcdo#caution_for
pcdo:caution_for rdf:type owl:ObjectProperty ;
                 rdfs:domain pcdo:Drug ;
                 rdfs:range pcdo:Special_Population .


###  http://sbmi.uth.edu/ontology/pcdo#comes_in
pcdo:comes_in rdf:type owl:ObjectProperty ;
              rdfs:domain pcdo:Drug ;
              rdfs:range pcdo:Form .


###  http://sbmi.uth.edu/ontology/pcdo#contains
pcdo:contains rdf:type owl:ObjectProperty ;
              rdfs:domain pcdo:Drug ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( pcdo:Allergen
                                         pcdo:Ingredients
                                       )
                         ] .


###  http://sbmi.uth.edu/ontology/pcdo#delivered_by
pcdo:delivered_by rdf:type owl:ObjectProperty ;
                  owl:inverseOf pcdo:delivered_in_the_form ;
                  rdfs:domain pcdo:Form ;
                  rdfs:range pcdo:Route .


###  http://sbmi.uth.edu/ontology/pcdo#delivered_in_the_form
pcdo:delivered_in_the_form rdf:type owl:ObjectProperty ;
                           rdfs:domain pcdo:Route ;
                           rdfs:range pcdo:Form .


###  http://sbmi.uth.edu/ontology/pcdo#discouraged_for
pcdo:discouraged_for rdf:type owl:ObjectProperty ;
                     rdfs:domain pcdo:Drug ;
                     rdfs:range pcdo:Special_Population .


###  http://sbmi.uth.edu/ontology/pcdo#dosage_spanning
pcdo:dosage_spanning rdf:type owl:ObjectProperty ;
                     rdfs:domain pcdo:Dosage ;
                     rdfs:range pcdo:Duration .


###  http://sbmi.uth.edu/ontology/pcdo#happens
pcdo:happens rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( pcdo:Cause
                                         pcdo:Outcome
                                         pcdo:Reaction
                                       )
                         ] ;
             rdfs:range pcdo:Probability .


###  http://sbmi.uth.edu/ontology/pcdo#has_alternate
pcdo:has_alternate rdf:type owl:ObjectProperty ;
                   rdfs:domain pcdo:Drug ;
                   rdfs:range pcdo:Alternate_Vaccine ;
                   rdfs:comment "Vaccine -> Alternate Vaccine" ;
                   skos:changeNote "consider removing or reconceputalizing" .


###  http://sbmi.uth.edu/ontology/pcdo#has_dosage
pcdo:has_dosage rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( pcdo:Dosage_Series
                                            pcdo:Drug
                                          )
                            ] ;
                rdfs:range pcdo:Dosage ;
                skos:changeNote "separate the domain to create a more expressive object property" .


###  http://sbmi.uth.edu/ontology/pcdo#has_dosage_series
pcdo:has_dosage_series rdf:type owl:ObjectProperty ;
                       rdfs:domain pcdo:Drug ;
                       rdfs:range pcdo:Dosage_Series .


###  http://sbmi.uth.edu/ontology/pcdo#has_dose_number_of
pcdo:has_dose_number_of rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain pcdo:Drug ;
                        rdfs:range pcdo:Number_of_Doses .


###  http://sbmi.uth.edu/ontology/pcdo#has_drug_interactions_with
pcdo:has_drug_interactions_with rdf:type owl:ObjectProperty ;
                                rdfs:domain pcdo:Drug_Combination ;
                                rdfs:range pcdo:Drug ,
                                           pcdo:Food ,
                                           pcdo:Form ,
                                           pcdo:Supplements .


###  http://sbmi.uth.edu/ontology/pcdo#has_risk_for
pcdo:has_risk_for rdf:type owl:ObjectProperty ;
                  rdfs:domain pcdo:Reaction ;
                  rdfs:range pcdo:Special_Population ;
                  owl:deprecated "true"^^xsd:boolean ;
                  skos:changeNote "same as \"affects individuals of\".. consider deleting" .


###  http://sbmi.uth.edu/ontology/pcdo#has_side_effect_symptoms_of
pcdo:has_side_effect_symptoms_of rdf:type owl:ObjectProperty ;
                                 rdfs:domain pcdo:Drug ;
                                 rdfs:range pcdo:Symptom ;
                                 rdfs:comment "Reaction->Signs" ;
                                 owl:deprecated "true"^^xsd:boolean .


###  http://sbmi.uth.edu/ontology/pcdo#has_signs
pcdo:has_signs rdf:type owl:ObjectProperty ;
               rdfs:domain pcdo:Reaction ;
               rdfs:range pcdo:Symptom ;
               rdfs:comment "Reaction->Signs" ;
               skos:changeNote """consider changing or adding Symptom (pcdo) instead of Sign

2022 - done""" .


###  http://sbmi.uth.edu/ontology/pcdo#has_special_instructions
pcdo:has_special_instructions rdf:type owl:ObjectProperty ;
                              rdfs:domain pcdo:Drug ;
                              rdfs:range pcdo:Special_instructions ;
                              skos:changeNote """maybe remove tradename concept

2022 - done""" .


###  http://sbmi.uth.edu/ontology/pcdo#involves_with
pcdo:involves_with rdf:type owl:ObjectProperty ;
                   rdfs:domain pcdo:Human_Activity_Channel ,
                               pcdo:People_Channel ;
                   rdfs:range pcdo:Special_Population ;
                   rdfs:comment "People Human Activity -> People" .


###  http://sbmi.uth.edu/ontology/pcdo#is_a_form_substance_of
pcdo:is_a_form_substance_of rdf:type owl:ObjectProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain pcdo:Form ;
                            owl:deprecated "true"^^xsd:boolean .


###  http://sbmi.uth.edu/ontology/pcdo#is_a_substance_of
pcdo:is_a_substance_of rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain pcdo:Drug ;
                       owl:deprecated "true"^^xsd:boolean .


###  http://sbmi.uth.edu/ontology/pcdo#is_also_known_as
pcdo:is_also_known_as rdf:type owl:ObjectProperty ;
                      rdfs:domain pcdo:Drug ;
                      rdfs:range pcdo:Tradename .


###  http://sbmi.uth.edu/ontology/pcdo#is_number_of_doses_for
pcdo:is_number_of_doses_for rdf:type owl:ObjectProperty ;
                            rdfs:domain pcdo:Number_of_Doses ;
                            rdfs:range pcdo:Special_Population ;
                            rdfs:comment "Number of Doses -> People" .


###  http://sbmi.uth.edu/ontology/pcdo#is_safe_for
pcdo:is_safe_for rdf:type owl:ObjectProperty ;
                 rdfs:domain pcdo:Drug ;
                 rdfs:range pcdo:Special_Population .


###  http://sbmi.uth.edu/ontology/pcdo#is_safe_with
pcdo:is_safe_with rdf:type owl:ObjectProperty ;
                  rdfs:domain pcdo:Drug ;
                  rdfs:range pcdo:Drug_Combination .


###  http://sbmi.uth.edu/ontology/pcdo#lasts
pcdo:lasts rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( pcdo:Drug_Target
                                       pcdo:Object_Channel
                                       pcdo:Reaction
                                       pcdo:Symptom
                                     )
                       ] ;
           rdfs:range pcdo:Duration .


###  http://sbmi.uth.edu/ontology/pcdo#leads
pcdo:leads rdf:type owl:ObjectProperty ;
           rdfs:range pcdo:Outcome ;
           rdfs:comment "Target -> Outome" ;
           rdfs:isDefinedBy "Cause -> Outcome" .


###  http://sbmi.uth.edu/ontology/pcdo#localized_at
pcdo:localized_at rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( pcdo:Cause
                                              pcdo:Reaction
                                            )
                              ] ;
                  rdfs:range pcdo:Body_Location .


###  http://sbmi.uth.edu/ontology/pcdo#may_cause
pcdo:may_cause rdf:type owl:ObjectProperty ;
               rdfs:domain pcdo:Drug ;
               rdfs:range pcdo:Reaction .


###  http://sbmi.uth.edu/ontology/pcdo#may_cause_with_drug_classess
pcdo:may_cause_with_drug_classess rdf:type owl:ObjectProperty ;
                                  rdfs:domain pcdo:Drug ;
                                  rdfs:range pcdo:Adverse_Drug_Reactions ;
                                  owl:deprecated "true"^^xsd:boolean ;
                                  skos:changeNote "redefine this to encapsulate drug classes and drug and adverse reaction" .


###  http://sbmi.uth.edu/ontology/pcdo#may_interact_with_drug_class
pcdo:may_interact_with_drug_class rdf:type owl:ObjectProperty ;
                                  rdfs:domain pcdo:Drug ;
                                  rdfs:range pcdo:Drug-Drug_Combination ;
                                  rdfs:comment "Reaction->Signs" ;
                                  owl:deprecated "true"^^xsd:boolean .


###  http://sbmi.uth.edu/ontology/pcdo#may_interact_with_drug_classes
pcdo:may_interact_with_drug_classes rdf:type owl:ObjectProperty ;
                                    rdfs:domain pcdo:Drug ;
                                    rdfs:range pcdo:Drug-Drug_Combination ;
                                    owl:deprecated "true"^^xsd:boolean .


###  http://sbmi.uth.edu/ontology/pcdo#may_result
pcdo:may_result rdf:type owl:ObjectProperty ;
                rdfs:domain pcdo:Drug_Target ;
                rdfs:range pcdo:Symptom .


###  http://sbmi.uth.edu/ontology/pcdo#need_to_take_action
pcdo:need_to_take_action rdf:type owl:ObjectProperty ;
                         rdfs:domain pcdo:Reaction ;
                         rdfs:range pcdo:Special_instructions ;
                         rdfs:comment "Reaction->Action" .


###  http://sbmi.uth.edu/ontology/pcdo#occurs_after
pcdo:occurs_after rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( pcdo:Reaction
                                              pcdo:Symptom
                                            )
                              ] ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( pcdo:Date
                                             pcdo:Occurrence
                                           )
                             ] ;
                  rdfs:comment "Reaction -> Occurence" .


###  http://sbmi.uth.edu/ontology/pcdo#of_age
pcdo:of_age rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain pcdo:People_of_Age ,
                        [ rdf:type owl:Class ;
                          owl:unionOf ( pcdo:Adults
                                        pcdo:Children
                                        pcdo:Infants
                                        pcdo:People_of_Age
                                      )
                        ] ;
            rdfs:range pcdo:Age .


###  http://sbmi.uth.edu/ontology/pcdo#originates
pcdo:originates rdf:type owl:ObjectProperty ;
                rdfs:domain pcdo:Drug_Target ;
                rdfs:range pcdo:Source .


###  http://sbmi.uth.edu/ontology/pcdo#personally_take
pcdo:personally_take rdf:type owl:ObjectProperty ;
                     rdfs:domain pcdo:Special_Population ;
                     rdfs:range pcdo:Special_instructions ;
                     rdfs:comment "People->Action" .


###  http://sbmi.uth.edu/ontology/pcdo#produces
pcdo:produces rdf:type owl:ObjectProperty ;
              rdfs:domain pcdo:Mechanism_of_Action ;
              rdfs:range pcdo:Drug_Effect_Outcome .


###  http://sbmi.uth.edu/ontology/pcdo#protects
pcdo:protects rdf:type owl:ObjectProperty ;
              rdfs:domain pcdo:Drug ;
              rdfs:range pcdo:Special_Population .


###  http://sbmi.uth.edu/ontology/pcdo#reaction_occurs_at
pcdo:reaction_occurs_at rdf:type owl:ObjectProperty ;
                        rdfs:domain pcdo:Reaction ;
                        rdfs:range pcdo:Body_Location ;
                        rdfs:comment "Reaction -> Location" ;
                        owl:deprecated "true"^^xsd:boolean ;
                        skos:changeNote "same as localized at" .


###  http://sbmi.uth.edu/ontology/pcdo#recommended_by
pcdo:recommended_by rdf:type owl:ObjectProperty ;
                    rdfs:domain pcdo:Drug ;
                    rdfs:range pcdo:Organization .


